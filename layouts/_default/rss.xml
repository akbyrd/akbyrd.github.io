{{ printf `<?xml version="1.0" encoding="utf-8" standalone="yes"?>` | safeHTML }}
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>{{ .Title }}</title>
		<language>{{ site.Language.LanguageCode }}</language>
		<lastBuildDate>{{ .Site.Lastmod.Format "02 Jan 2006" | safeHTML }}</lastBuildDate>
		<link>{{ .Permalink }}</link>
		{{ with .OutputFormats.Get "RSS" -}}
		{{ printf `<atom:link href=%q rel="self" type=%q />` .Permalink .MediaType | safeHTML -}}
		{{- end }}
		<image>
			<title>{{ .Title }}</title>
			<link>{{ .Permalink }}</link>
			<url>{{ .Site.Home.Permalink }}about/me.png</url>
		</image>
		{{- range .Pages }}

		<item>
			<title>{{ .Title }}</title>
			<link>{{ .Permalink }}</link>
			<guid>{{ .Permalink }}</guid>
			{{ with .PublishDate -}}
			<pubDate>{{ .Format "02 Jan 2006" | safeHTML }}</pubDate>
			{{ end -}}
		</item>
		{{- end }}
	</channel>
</rss>
